{
  "env": {
    "es6": true,
    "node": true,
    "cypress/globals": true
  },
  "extends": [
    "@teste/eslint-config-builders"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["cypress"]
}
